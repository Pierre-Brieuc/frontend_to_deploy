  console.log(movie._id);\n        console.log(this.token);\n        console.log(localStorage.getItem('user'));\n        console.log(API_URL);\n      });\n    },\n    activateModal(movie) {\n      this.showModal = true;\n      this.tempMovie = movie;\n      return {\n        sourceLocationIDPlaceholder: 'hhhhhh'\n      };\n    },\n    editfunction(movie) {\n      console.log(movie._id);\n      this.$router.push(`/editLocations/${movie._id}`);\n    },\n    changeInput(ref, refsend) {\n      if (this.$refs[ref].style.display === 'none') {\n        this.$refs[ref].style.display = 'block';\n        this.$refs[refsend].style.display = 'block';\n      } else {\n        this.$refs[ref].style.display = 'none';\n        this.$refs[refsend].style.display = 'none';\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAmDA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,KAAK,EAAEC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MACpCC,MAAM,EAAE,EAAE;MACVC,WAAW,EAAE,CAAC;MACdC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,KAAK;MAChBC,OAAO,EAAE,IAAI;MACbC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAY,GAAK;MACf,MAAMC,UAAS,GAAI,CAAC,IAAI,CAACP,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,OAAM;MACvD,OAAO,IAAI,CAACF,MAAM,CAACS,KAAK,CAACD,UAAU,EAAEA,UAAS,GAAI,IAAI,CAACN,OAAO;IAChE;EACF,CAAC;EACDQ,OAAM,GAAK;IACT,IAAI,IAAI,CAACC,YAAY,EAAE,EAAE;MACvB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ;IAC5B;IACA,IAAIf,YAAY,CAACC,OAAO,CAAC,OAAO,IAAI,CAAC,EAAE;MACrCe,QAAQ,CAACC,MAAM,EAAC;IAClB;IACAjB,YAAY,CAACkB,OAAO,CAAC,OAAO,EAAE,CAAC;IAC/BrB,KAAK,CAACsB,GAAG,CAAC,yDAAyD,EAAE;MACnEC,OAAO,EAAE;QACPC,aAAa,EAAG,UAAS,IAAI,CAACtB,KAAM;MACtC;IACF,CAAC,EACEuB,IAAI,CAACC,QAAO,IAAK;MAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACzB,IAAI;MACzB,IAAI,CAACI,MAAK,GAAIqB,QAAQ,CAACzB,IAAG;IAC5B,CAAC,EACA4B,KAAK,CAACC,KAAI,IAAK;MACdH,OAAO,CAACC,GAAG,CAACE,KAAK;IACnB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACPC,YAAW,GAAK;MACd;MACA,IAAI,IAAI,CAAC1B,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAC;MACnB;IACF,CAAC;IACD2B,QAAO,GAAK;MACV;MACA,IAAI,IAAI,CAAC3B,WA